                                         <th>Context</th>
                                         <th>actions</th>
                                        </tr>
                                   </thead>
                                   <tbody>
                                      {% for comment in comments %}
                                        <tr>
                                          <td>{{ comment.userName }}</td>
                                          <td>{{ comment.context }}</td>
                                          <td>
                                            <a href="{{ path('comment_delete', {'id': comment.id}) }}">delete /</a>
